{
  "server": {
  	"port": 7070,
  	"url": "http://ontosoft.isi.edu:7070"
  },
  "endpoints": {
    "enabled": [
      "okn-mint",
      "domain",
      "mint",
      "export",
      "wings"
    ],
    "domain": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://w3id.org",
      "path": "/wings/export/",
      "graph": true
    },
    "okn-mint": {
      "endpointQuery": "https://endpoint.mint.isi.edu/dev/query",
      "base": "https://w3id.org",
      "path": "/okn/i/mint"
    },
    "mint": {
      "endpointQuery": "https://endpoint.mint.isi.edu/ds/query",
      "base": "https://w3id.org",
      "path": "/mint/instance"
    },
    "export": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://www.opmw.org",
      "path": "/export/",
      "graph": true
    },
    "wings": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://w3id.org",
      "path": "wings"
    }
  }
}
